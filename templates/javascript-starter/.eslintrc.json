{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "sourceType": "module"
    },
    "globals": {
        "apex": false
    },
    "rules": {
        "no-console": [
            "warn"
        ],
        "indent": 0,
        "linebreak-style": 0,
        "arrow-body-style": 0,
        "implicit-arrow-linebreak": 0,
        "wrap-iife": 0,
        "max-len": 0,
        "import/prefer-default-export": 0,
        "import/no-unresolved": 0,
        "import/newline-after-import": 0
    }
}
